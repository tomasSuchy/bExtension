{
  "name": "Sreality",
  "description": "Extension is watching buildings informations on website sreality.cz based on customer filters",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "/popup/index.html",
    "default_icon": "/images/icon128.png"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "permissons": {
    "storage": ""
  }
}